/dts-v1/;

/* skeleton */
/ {
	#address-cells = <1>;
	#size-cells = <1>;
	chosen { };
	aliases { };
	memory { device_type = "memory"; reg = <0 0>; };
};

/* imx23 */
/ {
	interrupt-parent = <&icoll>;

	cpus {
		#address-cells = <0>;
		#size-cells = <0>;
		cpu {
			compatible = "arm,arm926ej-s";
			device_type = "cpu";
		};
	};

	apb@80000000 {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0x80000000 0x80000>;
		ranges;
		apbh@80000000 {
			compatible = "simple-bus";
			#address-cells = <1>;
			#size-cells = <1>;
			reg = <0x80000000 0x40000>;
			ranges;
			icoll: interrupt-controller@80000000 {
				compatible = "fsl,imx23-icoll", "fsl,icoll";
				interrupt-controller;
				#interrupt-cells = <1>;
				reg = <0x80000000 0x2000>;
			};
		};
	};
};

/* emulator */
/ {
	model = "i.MX233 Emulator";
	compatible = "fsl,imx23";

	memory {
		reg = <0x40000000 0x04000000>; /* 64 MiB */
	};
};

/* overridden */
/ {
	chosen {
		bootargs = "earlyprintk";
	};
};
